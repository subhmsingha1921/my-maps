        "backgroundColor": "#FFFFFF"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBsNne8pTEosdl6SsQeWo8d1lxc1pJidUg"
        }
      },
      "package": "com.subham_singha.mymaps"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-community-flipper"],
    "extra": {
      "eas": {
        "projectId": "48ed5b40-76ac-422d-901c-8e38d9dbcf2d"
      }
    }
  }
}
